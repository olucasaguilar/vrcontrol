<h1>Tipos de Entidade</h1>

<table class="table">
  <thead>
    <tr>
      <th>Nome</th>
      <th>Ações</th>
    </tr>
  </thead>
  <tbody>
    <% @entity_types.each do |entity_type| %>
      <tr>
        <td><%= entity_type.nome %></td>
        <td><a href="<%= filter_by_type_path(entity_type.id) %>" class="btn btn-primary">Ver entidades</a></td>
      </tr>
    <% end %>
  </tbody>
</table>